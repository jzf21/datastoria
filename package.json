{
  "name": "datastoria",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A modern, AI-powered ClickHouse management console that transforms how you interact with your data and manage your clusters",
  "scripts": {
    "postinstall": "npm run build:deps",
    "prebuild": "npm run build:deps",
    "build:deps": "npm run build:number-flow-core && npm run build:number-flow-react && npm run build:cmdk",
    "build:number-flow-core": "cd external/number-flow && npm install --include=dev --ignore-scripts && cd packages/number-flow && npm install --include=dev --ignore-scripts && npm run build",
    "build:number-flow-react": "cd external/number-flow && npm install --include=dev --ignore-scripts && cd packages/react && npm install --include=dev --ignore-scripts && npm run build",
    "build:cmdk": "cd external/cmdk/cmdk && npm install --include=dev --ignore-scripts && npm run build",
    "dev": "concurrently -n \"core,react,cmdk,app\" -c \"blue,green,cyan,yellow\" \"npm run dev:number-flow-core\" \"npm run dev:number-flow-react\" \"npm run dev:cmdk\" \"next dev\"",
    "dev:number-flow-core": "cd external/number-flow/packages/number-flow && npm run dev",
    "dev:number-flow-react": "cd external/number-flow/packages/react && npm run dev",
    "dev:cmdk": "cd external/cmdk/cmdk && rm -rf dist && npx tsup src --watch",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:all": "npm run format && npm run lint:fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/cerebras": "^2.0.2",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/groq": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@auth/core": "^0.37.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@number-flow/react": "file:./external/number-flow/packages/react",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
