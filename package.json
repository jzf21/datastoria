{
  "name": "datascopic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "AI-powered ClickHouse database management console with optional OAuth authentication",
  "scripts": {
    "postinstall": "npm run build:deps",
    "prebuild": "npm run build:deps",
    "build:deps": "npm run build:number-flow-core && npm run build:number-flow-react && npm run build:cmdk",
    "build:number-flow-core": "cd external/number-flow && npm install --include=dev --ignore-scripts && cd packages/number-flow && npm install --include=dev --ignore-scripts && npm run build",
    "build:number-flow-react": "cd external/number-flow && npm install --include=dev --ignore-scripts && cd packages/react && npm install --include=dev --ignore-scripts && npm run build",
    "build:cmdk": "cd external/cmdk/cmdk && npm install --include=dev --ignore-scripts && npm run build",
    "dev": "concurrently -n \"core,react,app\" -c \"blue,green,yellow\" \"npm run dev:number-flow-core\" \"npm run dev:number-flow-react\" \"next dev\"",
    "dev:number-flow-core": "cd external/number-flow/packages/number-flow && npm run dev",
    "dev:number-flow-react": "cd external/number-flow/packages/react && npm run dev",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.44",
    "@ai-sdk/google": "^2.0.31",
    "@ai-sdk/openai": "^2.0.65",
    "@ai-sdk/react": "^2.0.93",
    "@auth/core": "^0.37.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@number-flow/react": "file:./external/number-flow/packages/react",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
