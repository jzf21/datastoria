    "@tanstack/router-vite-plugin": "^1.134.6",
    "@types/dagre": "^0.7.53",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xyflow/react": "^12.9.2",
    "ace-builds": "npm:@frankchen029/ace-builds@^1.22.4",
    "ai": "^5.0.99",
    "ansi-to-html": "^0.7.2",
    "axios": "^1.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "file:./external/cmdk/cmdk",
