{
  "buildCommand": "npm run build",
  "installCommand": "npm install --include=dev --force",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "release": true,
      "master": true
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}

